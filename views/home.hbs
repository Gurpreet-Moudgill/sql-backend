<h1>hello</h1>
<a class="btn btn-success" href="/create">Create</a>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
     
    <tbody>
        <tr>
            {{#each product}}
            <th scope="row">{{this.id}}</th>
            <td>{{this.name}}</td>
            <td>{{this.email}}</td>
            <td>{{this.phoneNo}}</td>
            <td>
                <a class="btn btn-primary" href="products/getone/{{this.id}}">View</a>
                <a class="btn btn-success" href="#">Edit</a>
                <a class="btn btn-danger" href="products/del/{{this.id}}">Delete</a>
            </td>
        </tr>
  
     {{/each}} 
       </tbody>
</table>